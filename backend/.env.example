# Database
DATABASE_URL=postgresql://agent_user:agent_password@localhost:5432/agent_saas_db
POSTGRES_USER=agent_user
POSTGRES_PASSWORD=agent_password
POSTGRES_DB=agent_saas_db

# JWT Authentication
SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# API Keys - LLM Providers
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key-optional
OPENROUTER_API_KEY=your-openrouter-api-key
VOYAGE_API_KEY=your-voyage-api-key

# Voice & Assistant Integration
VAPI_API_KEY=your-vapi-api-key
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Qdrant Vector Database
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=agent_documents

# Application Settings
ENVIRONMENT=development
DEBUG=True
CORS_ORIGINS=http://localhost:8080,http://localhost:5173
MAX_UPLOAD_SIZE_MB=10

# Embeddings Configuration
EMBEDDING_MODEL=voyage-2
# Options: voyage-2, voyage-large-2, text-embedding-3-small, text-embedding-3-large
EMBEDDING_DIMENSION=1024

# Default LLM Configuration
DEFAULT_LLM_PROVIDER=openai
# Options: openai, anthropic, openrouter
DEFAULT_MODEL=gpt-4o-mini
# OpenAI: gpt-4o, gpt-4o-mini, gpt-3.5-turbo
# Anthropic: claude-3-5-sonnet-20241022, claude-3-haiku-20240307
# OpenRouter: Uses openai/ or anthropic/ prefixes
